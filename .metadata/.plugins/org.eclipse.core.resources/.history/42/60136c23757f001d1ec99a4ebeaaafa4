/*The position of account at the certain point of time. */
package org.museframework.bian.classes;

public class AccountPosition {
    /**/
    private String accountPositionDefinition;

    /*A classification of the position. 
*/
    private String positionType;

    /*The counted state (e.g., the amount of money, the value, the number) of something 
- It is the sum of entries
- It is the derived attribute
- It is an information object*/
    private String position;

    public void setAccountPositionDefinition(String accountPositionDefinition) {
        this.accountPositionDefinition = accountPositionDefinition;
    }

    public String getAccountPositionDefinition() {
        return accountPositionDefinition;
    }

    public void setPositionType(String positionType) {
        this.positionType = positionType;
    }

    public String getPositionType() {
        return positionType;
    }

    public void setPosition(String position) {
        this.position = position;
    }

    public String getPosition() {
        return position;
    }
}