/*A report resulting from market research. */
package org.museframework.bian.classes;

public class MarketResearchReport {
    /**/
    private String marketResearchReportContent;

    /*Reference to the Market Research Report Type which the report is an instance of*/
    private String marketResearchReportType;

    /*Period the Market research and its report apply to*/
    private String marketResearchReportPeriod;

    /*Version of the market research report*/
    private String marketResearchReportVersion;

    public void setMarketResearchReportContent(String marketResearchReportContent) {
        this.marketResearchReportContent = marketResearchReportContent;
    }

    public String getMarketResearchReportContent() {
        return marketResearchReportContent;
    }

    public void setMarketResearchReportType(String marketResearchReportType) {
        this.marketResearchReportType = marketResearchReportType;
    }

    public String getMarketResearchReportType() {
        return marketResearchReportType;
    }

    public void setMarketResearchReportPeriod(String marketResearchReportPeriod) {
        this.marketResearchReportPeriod = marketResearchReportPeriod;
    }

    public String getMarketResearchReportPeriod() {
        return marketResearchReportPeriod;
    }

    public void setMarketResearchReportVersion(String marketResearchReportVersion) {
        this.marketResearchReportVersion = marketResearchReportVersion;
    }

    public String getMarketResearchReportVersion() {
        return marketResearchReportVersion;
    }
}