/*An (structured) interaction between a customer and a financial institution.
*/
package org.museframework.bian.classes;

public class CustomerContact {
    /**/
    private String contactType;

    /*The start and end date times for the contact*/
    private String contactDateTime;

    /**/
    private Location contactLocation;

    /*The schedule of past and planned customer contacts*/
    private String contactSchedule;

    /*The interpreted result of the contact (e.g. completed, customer terminated, timed-out)*/
    private String contactResult;

    /*The reason and or purpose for the contact (e.g. review, issue, product sales, advice, customer enquiry)*/
    private String contactPurpose;

    /**/
    private Device contactDevice;

    /*Indicates whether this is an inbound or outbound contact (inbound is customer initiated, outbound is servicing resource initiated).*/
    private String contactIndicator;

    /**/
    private String contactContent;

    public void setContactType(String contactType) {
        this.contactType = contactType;
    }

    public String getContactType() {
        return contactType;
    }

    public void setContactDateTime(String contactDateTime) {
        this.contactDateTime = contactDateTime;
    }

    public String getContactDateTime() {
        return contactDateTime;
    }

    public void setContactLocation(Location contactLocation) {
        this.contactLocation = contactLocation;
    }

    public Location getContactLocation() {
        return contactLocation;
    }

    public void setContactSchedule(String contactSchedule) {
        this.contactSchedule = contactSchedule;
    }

    public String getContactSchedule() {
        return contactSchedule;
    }

    public void setContactResult(String contactResult) {
        this.contactResult = contactResult;
    }

    public String getContactResult() {
        return contactResult;
    }

    public void setContactPurpose(String contactPurpose) {
        this.contactPurpose = contactPurpose;
    }

    public String getContactPurpose() {
        return contactPurpose;
    }

    public void setContactDevice(Device contactDevice) {
        this.contactDevice = contactDevice;
    }

    public Device getContactDevice() {
        return contactDevice;
    }

    public void setContactIndicator(String contactIndicator) {
        this.contactIndicator = contactIndicator;
    }

    public String getContactIndicator() {
        return contactIndicator;
    }

    public void setContactContent(String contactContent) {
        this.contactContent = contactContent;
    }

    public String getContactContent() {
        return contactContent;
    }
}