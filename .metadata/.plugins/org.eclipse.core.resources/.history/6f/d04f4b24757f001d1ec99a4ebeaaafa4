/**/
package org.museframework.bian.classes;

public class ProductSalesTermAndCondition {
    /**/
    private String productSalesTermAndConditionType;

    /**/
    private String productSalesTermAndConditionDescription;

    public void setProductSalesTermAndConditionType(String productSalesTermAndConditionType) {
        this.productSalesTermAndConditionType = productSalesTermAndConditionType;
    }

    public String getProductSalesTermAndConditionType() {
        return productSalesTermAndConditionType;
    }

    public void setProductSalesTermAndConditionDescription(String productSalesTermAndConditionDescription) {
        this.productSalesTermAndConditionDescription = productSalesTermAndConditionDescription;
    }

    public String getProductSalesTermAndConditionDescription() {
        return productSalesTermAndConditionDescription;
    }
}