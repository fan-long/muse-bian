/*A binding rule that the courts will enforce.
A system of rules and guidelines which are enforced through social institutions to govern behavior. (FIBO)
*/
package org.museframework.bian.classes;

public class Law {
    /**/
    private String lawType;

    /**/
    private String lawDescription;

    /*the possible implications of breaking the law*/
    private String legalPenalty;

    /*A guidelines and requirement for following the law*/
    private String legalGuideline;

    public void setLawType(String lawType) {
        this.lawType = lawType;
    }

    public String getLawType() {
        return lawType;
    }

    public void setLawDescription(String lawDescription) {
        this.lawDescription = lawDescription;
    }

    public String getLawDescription() {
        return lawDescription;
    }

    public void setLegalPenalty(String legalPenalty) {
        this.legalPenalty = legalPenalty;
    }

    public String getLegalPenalty() {
        return legalPenalty;
    }

    public void setLegalGuideline(String legalGuideline) {
        this.legalGuideline = legalGuideline;
    }

    public String getLegalGuideline() {
        return legalGuideline;
    }
}