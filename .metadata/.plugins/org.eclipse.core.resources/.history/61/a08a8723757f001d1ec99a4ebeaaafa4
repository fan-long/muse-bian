/*A specific role of a specific Party in the specific relationship.  */
package org.museframework.bian.classes;

public class PartyRole {
    /**/
    private String partyRoleType;

    /**/
    private String partyRoleName;

    /**/
    private String partyRoleValidityPeriod;

    /**/
    private org.museframework.bian.enumerations.Partyinvolvementtypevalues partyInvolvementType;

    public void setPartyRoleType(String partyRoleType) {
        this.partyRoleType = partyRoleType;
    }

    public String getPartyRoleType() {
        return partyRoleType;
    }

    public void setPartyRoleName(String partyRoleName) {
        this.partyRoleName = partyRoleName;
    }

    public String getPartyRoleName() {
        return partyRoleName;
    }

    public void setPartyRoleValidityPeriod(String partyRoleValidityPeriod) {
        this.partyRoleValidityPeriod = partyRoleValidityPeriod;
    }

    public String getPartyRoleValidityPeriod() {
        return partyRoleValidityPeriod;
    }

    public void setPartyInvolvementType(org.museframework.bian.enumerations.Partyinvolvementtypevalues partyInvolvementType) {
        this.partyInvolvementType = partyInvolvementType;
    }

    public org.museframework.bian.enumerations.Partyinvolvementtypevalues getPartyInvolvementType() {
        return partyInvolvementType;
    }
}