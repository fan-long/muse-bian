/*An arrangement concerning the information the bank will provide about the agreement/account*/
package org.museframework.bian.classes;

public class InformationArrangement {
    /*Type of statement agreed upon

e.g. balance, mini-statement, annual */
    private String accountStatementType;

    /*Definition of the timetable for providing statements

e.g. start date and frequency, or a list of dates or "on demand"*/
    private String accountStatementSchedule;

    /*Content type to be included in the statements 

e.g. type of transactions and transaction details*/
    private String accountStatementContenttype;

    public void setAccountStatementType(String accountStatementType) {
        this.accountStatementType = accountStatementType;
    }

    public String getAccountStatementType() {
        return accountStatementType;
    }

    public void setAccountStatementSchedule(String accountStatementSchedule) {
        this.accountStatementSchedule = accountStatementSchedule;
    }

    public String getAccountStatementSchedule() {
        return accountStatementSchedule;
    }

    public void setAccountStatementContenttype(String accountStatementContenttype) {
        this.accountStatementContenttype = accountStatementContenttype;
    }

    public String getAccountStatementContenttype() {
        return accountStatementContenttype;
    }
}