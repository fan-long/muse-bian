/*Task to determine the final resolution of the collections process - either the payments are received account is recovered or it is passed to recovery processing. */
package org.museframework.bian.classes;

public class CollectionResolutionWorkstep {
    /*Details of the resolution decision. */
    private String resolutionDecision;

    /*Work documentation, forms and schedules produced and referenced during the task. */
    private String resolutionWorkProduct;

    public void setResolutionDecision(String resolutionDecision) {
        this.resolutionDecision = resolutionDecision;
    }

    public String getResolutionDecision() {
        return resolutionDecision;
    }

    public void setResolutionWorkProduct(String resolutionWorkProduct) {
        this.resolutionWorkProduct = resolutionWorkProduct;
    }

    public String getResolutionWorkProduct() {
        return resolutionWorkProduct;
    }
}