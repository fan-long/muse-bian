/*An action, situation, or decision that has already happened (for a customer) and can be used as a reason why a similar action or decision should be performed or made. */
package org.museframework.bian.classes;

public class CustomerPrecedent {
    /*e.g. residential status, marital status, military, in a disaster location, etc.*/
    private String customerPrecedentType;

    /**/
    private String customerPrecedentValue;

    /**/
    private String customerPrecedentValueDate;

    public void setCustomerPrecedentType(String customerPrecedentType) {
        this.customerPrecedentType = customerPrecedentType;
    }

    public String getCustomerPrecedentType() {
        return customerPrecedentType;
    }

    public void setCustomerPrecedentValue(String customerPrecedentValue) {
        this.customerPrecedentValue = customerPrecedentValue;
    }

    public String getCustomerPrecedentValue() {
        return customerPrecedentValue;
    }

    public void setCustomerPrecedentValueDate(String customerPrecedentValueDate) {
        this.customerPrecedentValueDate = customerPrecedentValueDate;
    }

    public String getCustomerPrecedentValueDate() {
        return customerPrecedentValueDate;
    }
}