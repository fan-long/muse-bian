/*An instruction to do something expressed in an amount of money.*/
package org.museframework.bian.classes;

public class FinancialInstruction {
    /**/
    private String instructionAmount;

    /**/
    private String instructionCurrency;

    /**/
    private org.museframework.bian.enumerations.Financialinstructiontypevalues financialInstructionType;

    public void setInstructionAmount(String instructionAmount) {
        this.instructionAmount = instructionAmount;
    }

    public String getInstructionAmount() {
        return instructionAmount;
    }

    public void setInstructionCurrency(String instructionCurrency) {
        this.instructionCurrency = instructionCurrency;
    }

    public String getInstructionCurrency() {
        return instructionCurrency;
    }

    public void setFinancialInstructionType(org.museframework.bian.enumerations.Financialinstructiontypevalues financialInstructionType) {
        this.financialInstructionType = financialInstructionType;
    }

    public org.museframework.bian.enumerations.Financialinstructiontypevalues getFinancialInstructionType() {
        return financialInstructionType;
    }
}