/*Restriction on capability or operations allowed. (ISO20022)*/
package org.museframework.bian.classes;

public class AccountRestiction {
    /*Identifier of the restriction defined on the account.*/
    private String accountRestrictionIdentifier;

    /*Type of the restriction.*/
    private String accountRestrictionType;

    /*Description of the restriction specified on the account.*/
    private String accountRestrictionDescription;

    public void setAccountRestrictionIdentifier(String accountRestrictionIdentifier) {
        this.accountRestrictionIdentifier = accountRestrictionIdentifier;
    }

    public String getAccountRestrictionIdentifier() {
        return accountRestrictionIdentifier;
    }

    public void setAccountRestrictionType(String accountRestrictionType) {
        this.accountRestrictionType = accountRestrictionType;
    }

    public String getAccountRestrictionType() {
        return accountRestrictionType;
    }

    public void setAccountRestrictionDescription(String accountRestrictionDescription) {
        this.accountRestrictionDescription = accountRestrictionDescription;
    }

    public String getAccountRestrictionDescription() {
        return accountRestrictionDescription;
    }
}