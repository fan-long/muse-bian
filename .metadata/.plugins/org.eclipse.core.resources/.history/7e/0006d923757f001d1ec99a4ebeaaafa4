/*Entry in a document directory. */
package org.museframework.bian.classes;

public class DocumentDirectoryEntry {
    /*Identification allowing the retrieval of a document from a Document Directory*/
    private String documentDirectoryEntryIdentification;

    public void setDocumentDirectoryEntryIdentification(String documentDirectoryEntryIdentification) {
        this.documentDirectoryEntryIdentification = documentDirectoryEntryIdentification;
    }

    public String getDocumentDirectoryEntryIdentification() {
        return documentDirectoryEntryIdentification;
    }
}