/*The specific role that a specific party has in a specific payment instruction */
package org.museframework.bian.classes;

public class MoneyTransferInstructionInvolvement {
    /*The role a pary plays in a Money Transfer Instruction*/
    private String moneyTransferInstructionInvolvementType;

    /*The party that plays the role in the instruction*/
    private String moneyTransferInstructionInvolvedPartyReference;

    /*Reference to the instruction the party is involved in.*/
    private String moneyTransferInstructionReference;

    public void setMoneyTransferInstructionInvolvementType(String moneyTransferInstructionInvolvementType) {
        this.moneyTransferInstructionInvolvementType = moneyTransferInstructionInvolvementType;
    }

    public String getMoneyTransferInstructionInvolvementType() {
        return moneyTransferInstructionInvolvementType;
    }

    public void setMoneyTransferInstructionInvolvedPartyReference(String moneyTransferInstructionInvolvedPartyReference) {
        this.moneyTransferInstructionInvolvedPartyReference = moneyTransferInstructionInvolvedPartyReference;
    }

    public String getMoneyTransferInstructionInvolvedPartyReference() {
        return moneyTransferInstructionInvolvedPartyReference;
    }

    public void setMoneyTransferInstructionReference(String moneyTransferInstructionReference) {
        this.moneyTransferInstructionReference = moneyTransferInstructionReference;
    }

    public String getMoneyTransferInstructionReference() {
        return moneyTransferInstructionReference;
    }
}