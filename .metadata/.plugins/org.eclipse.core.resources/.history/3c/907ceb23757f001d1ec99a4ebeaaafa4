/*A Loan Product is a Product offered by a lender (financial institution) to the market, into which money can be borrowed under specific terms and conditions.*/
package org.museframework.bian.classes;

public class LoanProduct {
    /*A desired Amount of loan. */
    private String loanAmount;

    /*Purpose of loan.*/
    private String loanPurpose;

    /*Identifier of loan product.*/
    private String loanProductIdentifier;

    /**/
    private org.museframework.bian.enumerations.Loanproducttypevalues loanProductType;

    public void setLoanAmount(String loanAmount) {
        this.loanAmount = loanAmount;
    }

    public String getLoanAmount() {
        return loanAmount;
    }

    public void setLoanPurpose(String loanPurpose) {
        this.loanPurpose = loanPurpose;
    }

    public String getLoanPurpose() {
        return loanPurpose;
    }

    public void setLoanProductIdentifier(String loanProductIdentifier) {
        this.loanProductIdentifier = loanProductIdentifier;
    }

    public String getLoanProductIdentifier() {
        return loanProductIdentifier;
    }

    public void setLoanProductType(org.museframework.bian.enumerations.Loanproducttypevalues loanProductType) {
        this.loanProductType = loanProductType;
    }

    public org.museframework.bian.enumerations.Loanproducttypevalues getLoanProductType() {
        return loanProductType;
    }
}