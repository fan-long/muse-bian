/*A point in time. 
Can be a period, a date or a more detailed time indication.*/
package org.museframework.bian.classes;

public class DateTime {
    /*Expression of the point in time

is expressed according to "TimeIndicationType"
e.g. period, day, more detailed indication*/
    private String timeIndication;

    /**/
    private String timeIndicationType;

    public void setTimeIndication(String timeIndication) {
        this.timeIndication = timeIndication;
    }

    public String getTimeIndication() {
        return timeIndication;
    }

    public void setTimeIndicationType(String timeIndicationType) {
        this.timeIndicationType = timeIndicationType;
    }

    public String getTimeIndicationType() {
        return timeIndicationType;
    }
}