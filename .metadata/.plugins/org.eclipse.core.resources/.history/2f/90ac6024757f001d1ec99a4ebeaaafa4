/*Report concerning a financial viewpoint on the bank

e.g. Consolidated cashflow, liquidity position, treasury position, (enterprise) balance, list of transactions above a certain amount...*/
package org.museframework.bian.classes;

public class FinancialReport {
    /*The viewpoint used in the financial report

e.g. Consolidated cashflow, liquidity position, treasury position, (enterprise) balance , Assets and liabilities*/
    private String financialreportViewpoint;

    /*The parts of the enterprise taken into account for the report

e.g. group, enterprise, business unit...
loans, deposits..., retail, corporate...*/
    private String financialReportScope;

    /**/
    private String attribute;

    public void setFinancialreportViewpoint(String financialreportViewpoint) {
        this.financialreportViewpoint = financialreportViewpoint;
    }

    public String getFinancialreportViewpoint() {
        return financialreportViewpoint;
    }

    public void setFinancialReportScope(String financialReportScope) {
        this.financialReportScope = financialReportScope;
    }

    public String getFinancialReportScope() {
        return financialReportScope;
    }

    public void setAttribute(String attribute) {
        this.attribute = attribute;
    }

    public String getAttribute() {
        return attribute;
    }
}