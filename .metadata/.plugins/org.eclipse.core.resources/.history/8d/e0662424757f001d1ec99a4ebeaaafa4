/*A request for trading (e.g., buy, sell) the securities 

Intention to transfer an ownership of a financial instrument. (ISO20022)*/
package org.museframework.bian.classes;

public class SecuritiesOrder {
    /*)*/
    private String securitiesOrderType;

    public void setSecuritiesOrderType(String securitiesOrderType) {
        this.securitiesOrderType = securitiesOrderType;
    }

    public String getSecuritiesOrderType() {
        return securitiesOrderType;
    }
}