/*Reference to a product agreement for which the overdue accounts receivable need to be collected*/
package org.museframework.bian.classes;

public class DunningSubject {
    /*The Product Sales Agreement for which the overdue accounts receivable are being collected*/
    private String dunningSubjectReference;

    public void setDunningSubjectReference(String dunningSubjectReference) {
        this.dunningSubjectReference = dunningSubjectReference;
    }

    public String getDunningSubjectReference() {
        return dunningSubjectReference;
    }
}