/*Tracking of the state and/or activities of a device */
package org.museframework.bian.classes;

public class IssuedDeviceMeasurement {
    /*Date/Time on which the Measurement of the issued Device took place*/
    private String deviceMeasurementDateTime;

    /**/
    private String deviceMeasurementDetails;

    /**/
    private String deviceMeasurementResult;

    /*Reference of the source of the issued device measurement*/
    private String informationFeedReference;

    public void setDeviceMeasurementDateTime(String deviceMeasurementDateTime) {
        this.deviceMeasurementDateTime = deviceMeasurementDateTime;
    }

    public String getDeviceMeasurementDateTime() {
        return deviceMeasurementDateTime;
    }

    public void setDeviceMeasurementDetails(String deviceMeasurementDetails) {
        this.deviceMeasurementDetails = deviceMeasurementDetails;
    }

    public String getDeviceMeasurementDetails() {
        return deviceMeasurementDetails;
    }

    public void setDeviceMeasurementResult(String deviceMeasurementResult) {
        this.deviceMeasurementResult = deviceMeasurementResult;
    }

    public String getDeviceMeasurementResult() {
        return deviceMeasurementResult;
    }

    public void setInformationFeedReference(String informationFeedReference) {
        this.informationFeedReference = informationFeedReference;
    }

    public String getInformationFeedReference() {
        return informationFeedReference;
    }
}