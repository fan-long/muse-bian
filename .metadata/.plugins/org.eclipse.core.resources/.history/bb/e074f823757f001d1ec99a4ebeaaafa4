/*The revelation of information relevant to an agreement or potential agreement.*/
package org.museframework.bian.classes;

public class Disclosure {
    /**/
    private String disclosureType;

    /**/
    private String confirmationIndicator;

    /**/
    private Document specifiedDocument;

    public void setDisclosureType(String disclosureType) {
        this.disclosureType = disclosureType;
    }

    public String getDisclosureType() {
        return disclosureType;
    }

    public void setConfirmationIndicator(String confirmationIndicator) {
        this.confirmationIndicator = confirmationIndicator;
    }

    public String getConfirmationIndicator() {
        return confirmationIndicator;
    }

    public void setSpecifiedDocument(Document specifiedDocument) {
        this.specifiedDocument = specifiedDocument;
    }

    public Document getSpecifiedDocument() {
        return specifiedDocument;
    }
}