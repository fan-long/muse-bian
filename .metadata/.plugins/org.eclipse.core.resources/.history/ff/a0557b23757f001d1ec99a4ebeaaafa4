/*Specification of how a tax handling arrangement can be fulfilled.
*/
package org.museframework.bian.classes;

public class TaxHandlingArrangementModality {
    /**/
    private String taxReportingSchedule;

    public void setTaxReportingSchedule(String taxReportingSchedule) {
        this.taxReportingSchedule = taxReportingSchedule;
    }

    public String getTaxReportingSchedule() {
        return taxReportingSchedule;
    }
}