/*Configuration for deployment a model.*/
package org.museframework.bian.classes;

public class ModelDeploymentConfiguration {
    /*Reference to the specification deployment task*/
    private String modelDeploymentTask;

    /*Party who plays the role in model deployment. 
For example, the business units where the product or service is in use or supported. */
    private String modelDeploymentInvolvement;

    /*The operational and development status of the model (e.g. under development, available, under review).*/
    private String modelDeploymentStatus;

    public void setModelDeploymentTask(String modelDeploymentTask) {
        this.modelDeploymentTask = modelDeploymentTask;
    }

    public String getModelDeploymentTask() {
        return modelDeploymentTask;
    }

    public void setModelDeploymentInvolvement(String modelDeploymentInvolvement) {
        this.modelDeploymentInvolvement = modelDeploymentInvolvement;
    }

    public String getModelDeploymentInvolvement() {
        return modelDeploymentInvolvement;
    }

    public void setModelDeploymentStatus(String modelDeploymentStatus) {
        this.modelDeploymentStatus = modelDeploymentStatus;
    }

    public String getModelDeploymentStatus() {
        return modelDeploymentStatus;
    }
}