/*Term and condition to govern a product profile.*/
package org.museframework.bian.classes;

public class ProductProfileTermAndCondition {
    /**/
    private String productProfileTermAndConditionType;

    public void setProductProfileTermAndConditionType(String productProfileTermAndConditionType) {
        this.productProfileTermAndConditionType = productProfileTermAndConditionType;
    }

    public String getProductProfileTermAndConditionType() {
        return productProfileTermAndConditionType;
    }
}