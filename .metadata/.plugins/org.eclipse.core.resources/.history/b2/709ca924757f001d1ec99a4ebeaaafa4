/*The distribution of product inventory to a location*/
package org.museframework.bian.classes;

public class ProductInventoryAssignment {
    /*Details of the distribution of product inventory to a location*/
    private String productInventoryAssignmentDetail;

    /*Details of inventory and delivery related costs*/
    private String productInventoryAssignmentCost;

    public void setProductInventoryAssignmentDetail(String productInventoryAssignmentDetail) {
        this.productInventoryAssignmentDetail = productInventoryAssignmentDetail;
    }

    public String getProductInventoryAssignmentDetail() {
        return productInventoryAssignmentDetail;
    }

    public void setProductInventoryAssignmentCost(String productInventoryAssignmentCost) {
        this.productInventoryAssignmentCost = productInventoryAssignmentCost;
    }

    public String getProductInventoryAssignmentCost() {
        return productInventoryAssignmentCost;
    }
}