/*to do: definitie opzoeken

A statement of account is a detailed report of the contents of an account. (AccountingTool)*/
package org.museframework.bian.classes;

public class AccountStatement {
    /*a Classification of the balance that is shown on a statement

e.g. booked, available, closing... */
    private String accountStatementBalanceType;

    /*The balance (of balance type) that is shown on an account statement*/
    private String accounStatementBalance;

    /*Unique identification of an account statement*/
    private String accounStatementIdentification;

    /*Classification of account statement according to ....  */
    private String accountStatementType;

    /*classification of the content that is mentioned on an account statement*/
    private String accountStatementContentType;

    /*Period about which an account statement provides information*/
    private String acountStatementPeriod;

    /*The information contained by the account statement */
    private String accountStatementContent;

    public void setAccountStatementBalanceType(String accountStatementBalanceType) {
        this.accountStatementBalanceType = accountStatementBalanceType;
    }

    public String getAccountStatementBalanceType() {
        return accountStatementBalanceType;
    }

    public void setAccounStatementBalance(String accounStatementBalance) {
        this.accounStatementBalance = accounStatementBalance;
    }

    public String getAccounStatementBalance() {
        return accounStatementBalance;
    }

    public void setAccounStatementIdentification(String accounStatementIdentification) {
        this.accounStatementIdentification = accounStatementIdentification;
    }

    public String getAccounStatementIdentification() {
        return accounStatementIdentification;
    }

    public void setAccountStatementType(String accountStatementType) {
        this.accountStatementType = accountStatementType;
    }

    public String getAccountStatementType() {
        return accountStatementType;
    }

    public void setAccountStatementContentType(String accountStatementContentType) {
        this.accountStatementContentType = accountStatementContentType;
    }

    public String getAccountStatementContentType() {
        return accountStatementContentType;
    }

    public void setAcountStatementPeriod(String acountStatementPeriod) {
        this.acountStatementPeriod = acountStatementPeriod;
    }

    public String getAcountStatementPeriod() {
        return acountStatementPeriod;
    }

    public void setAccountStatementContent(String accountStatementContent) {
        this.accountStatementContent = accountStatementContent;
    }

    public String getAccountStatementContent() {
        return accountStatementContent;
    }
}