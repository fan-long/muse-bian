/*The application of the Market Analysis Results in management decisions*/
package org.museframework.bian.classes;

public class MarketAnalysisUsage {
    /*The employee or organisational unit using the market analysis*/
    private String marketAnalysisUser;

    /*Reported consequences of the use of the market analysis

e.g. decisions*/
    private String marketAnalysysImpact;

    /*Reported evaluation of the analysis

e.g. accuracy*/
    private String marketAnalysisFeedback;

    public void setMarketAnalysisUser(String marketAnalysisUser) {
        this.marketAnalysisUser = marketAnalysisUser;
    }

    public String getMarketAnalysisUser() {
        return marketAnalysisUser;
    }

    public void setMarketAnalysysImpact(String marketAnalysysImpact) {
        this.marketAnalysysImpact = marketAnalysysImpact;
    }

    public String getMarketAnalysysImpact() {
        return marketAnalysysImpact;
    }

    public void setMarketAnalysisFeedback(String marketAnalysisFeedback) {
        this.marketAnalysisFeedback = marketAnalysisFeedback;
    }

    public String getMarketAnalysisFeedback() {
        return marketAnalysisFeedback;
    }
}