/*Feedback given by a customer in the context of a dunning procedure. */
package org.museframework.bian.classes;

public class DunningCustomerFeedback {
    /*Identification of the customer feedback*/
    private String dunningCustomerFeedbackIdentification;

    /*The format channel, medium, interaction modus, lay-out...) of the customer feedback

e.g. phone conversation, letter, fill-in form */
    private String dunningCustomerFeedbackFormat;

    /*Content of the customer feedback*/
    private String dunningCustomerFeedbackContent;

    /*Date and Time at which the customer gave the feedback*/
    private String dunningCustomerFeedbackDateTime;

    /*Status of the recording process of the customer feedback
Or: processing status? 
Status in processing the feedback, e.g. reaction of dunning contact person*/
    private String dunningCustomerFeedbackRecordingOrReactionStatus;

    /*Employee who recorded the customer feedback*/
    private String dunningCustomerFeedbackEmployeeReference;

    public void setDunningCustomerFeedbackIdentification(String dunningCustomerFeedbackIdentification) {
        this.dunningCustomerFeedbackIdentification = dunningCustomerFeedbackIdentification;
    }

    public String getDunningCustomerFeedbackIdentification() {
        return dunningCustomerFeedbackIdentification;
    }

    public void setDunningCustomerFeedbackFormat(String dunningCustomerFeedbackFormat) {
        this.dunningCustomerFeedbackFormat = dunningCustomerFeedbackFormat;
    }

    public String getDunningCustomerFeedbackFormat() {
        return dunningCustomerFeedbackFormat;
    }

    public void setDunningCustomerFeedbackContent(String dunningCustomerFeedbackContent) {
        this.dunningCustomerFeedbackContent = dunningCustomerFeedbackContent;
    }

    public String getDunningCustomerFeedbackContent() {
        return dunningCustomerFeedbackContent;
    }

    public void setDunningCustomerFeedbackDateTime(String dunningCustomerFeedbackDateTime) {
        this.dunningCustomerFeedbackDateTime = dunningCustomerFeedbackDateTime;
    }

    public String getDunningCustomerFeedbackDateTime() {
        return dunningCustomerFeedbackDateTime;
    }

    public void setDunningCustomerFeedbackRecordingOrReactionStatus(String dunningCustomerFeedbackRecordingOrReactionStatus) {
        this.dunningCustomerFeedbackRecordingOrReactionStatus = dunningCustomerFeedbackRecordingOrReactionStatus;
    }

    public String getDunningCustomerFeedbackRecordingOrReactionStatus() {
        return dunningCustomerFeedbackRecordingOrReactionStatus;
    }

    public void setDunningCustomerFeedbackEmployeeReference(String dunningCustomerFeedbackEmployeeReference) {
        this.dunningCustomerFeedbackEmployeeReference = dunningCustomerFeedbackEmployeeReference;
    }

    public String getDunningCustomerFeedbackEmployeeReference() {
        return dunningCustomerFeedbackEmployeeReference;
    }
}