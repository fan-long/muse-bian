/*Document that will be assigned to the agreement. */
package org.museframework.bian.classes;

public class AgreementDocument {
    /*Refernce to the relevant document */
    private Document documentReference;

    /**/
    private Agreement agreementReference;

    public void setDocumentReference(Document documentReference) {
        this.documentReference = documentReference;
    }

    public Document getDocumentReference() {
        return documentReference;
    }

    public void setAgreementReference(Agreement agreementReference) {
        this.agreementReference = agreementReference;
    }

    public Agreement getAgreementReference() {
        return agreementReference;
    }
}