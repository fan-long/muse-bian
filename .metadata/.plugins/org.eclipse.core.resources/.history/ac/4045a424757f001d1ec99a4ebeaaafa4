/*Specifies the location of document. */
package org.museframework.bian.classes;

public class DocumentLocation {
    /**/
    private String documentLocationIdentification;

    /**/
    private Document documentReference;

    /**/
    private Location locationReference;

    /**/
    private org.museframework.bian.enumerations.Documentlocationtypevalues documentLocationType;

    public void setDocumentLocationIdentification(String documentLocationIdentification) {
        this.documentLocationIdentification = documentLocationIdentification;
    }

    public String getDocumentLocationIdentification() {
        return documentLocationIdentification;
    }

    public void setDocumentReference(Document documentReference) {
        this.documentReference = documentReference;
    }

    public Document getDocumentReference() {
        return documentReference;
    }

    public void setLocationReference(Location locationReference) {
        this.locationReference = locationReference;
    }

    public Location getLocationReference() {
        return locationReference;
    }

    public void setDocumentLocationType(org.museframework.bian.enumerations.Documentlocationtypevalues documentLocationType) {
        this.documentLocationType = documentLocationType;
    }

    public org.museframework.bian.enumerations.Documentlocationtypevalues getDocumentLocationType() {
        return documentLocationType;
    }
}