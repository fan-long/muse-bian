/*A responsibility of task (under management).

Duties are a collection of one or more responsibilities or tasks under management. (Behavior Qualifier Type)*/
package org.museframework.bian.classes;

public class Duty {
    /**/
    private String dutyType;

    /**/
    private String dutyDescription;

    /*The outcome or result of the work task */
    private String dutyResult;

    /*Key dates associated with the work task*/
    private String dutyDateTime;

    /*File of consolidated notes, evaluations and recommendations for the work task*/
    private String dutyWorkProduct;

    public void setDutyType(String dutyType) {
        this.dutyType = dutyType;
    }

    public String getDutyType() {
        return dutyType;
    }

    public void setDutyDescription(String dutyDescription) {
        this.dutyDescription = dutyDescription;
    }

    public String getDutyDescription() {
        return dutyDescription;
    }

    public void setDutyResult(String dutyResult) {
        this.dutyResult = dutyResult;
    }

    public String getDutyResult() {
        return dutyResult;
    }

    public void setDutyDateTime(String dutyDateTime) {
        this.dutyDateTime = dutyDateTime;
    }

    public String getDutyDateTime() {
        return dutyDateTime;
    }

    public void setDutyWorkProduct(String dutyWorkProduct) {
        this.dutyWorkProduct = dutyWorkProduct;
    }

    public String getDutyWorkProduct() {
        return dutyWorkProduct;
    }
}