/*A design specification defined for a standard (e.g. Technology Standard, Data Standard). */
package org.museframework.bian.classes;

public class StandardDesignSpecification {
    /**/
    private String standardDesignSpecificationType;

    public void setStandardDesignSpecificationType(String standardDesignSpecificationType) {
        this.standardDesignSpecificationType = standardDesignSpecificationType;
    }

    public String getStandardDesignSpecificationType() {
        return standardDesignSpecificationType;
    }
}