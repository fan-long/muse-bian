/*An analysis which is applied on an individual transaction detected as fraud to support an appropriate response/recommendation. */
package org.museframework.bian.classes;

public class FraudDiagnosis {
    /*Details the conclusion of the analysis and determination (e.g. diagnosis of likely fraud).*/
    private String determination;

    /**/
    private String recommendation;

    public void setDetermination(String determination) {
        this.determination = determination;
    }

    public String getDetermination() {
        return determination;
    }

    public void setRecommendation(String recommendation) {
        this.recommendation = recommendation;
    }

    public String getRecommendation() {
        return recommendation;
    }
}