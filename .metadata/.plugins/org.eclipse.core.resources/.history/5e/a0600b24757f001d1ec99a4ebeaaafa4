/*A procedure to allocate partially completed market orders
*/
package org.museframework.bian.classes;

public class SecuritiesAllocation {
    /**/
    private Workstep securitiesAllocationWorkstep;

    /**/
    private org.museframework.bian.enumerations.Securitiesallocationworksteptypevalues securitiesAllocationWorkstepType;

    public void setSecuritiesAllocationWorkstep(Workstep securitiesAllocationWorkstep) {
        this.securitiesAllocationWorkstep = securitiesAllocationWorkstep;
    }

    public Workstep getSecuritiesAllocationWorkstep() {
        return securitiesAllocationWorkstep;
    }

    public void setSecuritiesAllocationWorkstepType(org.museframework.bian.enumerations.Securitiesallocationworksteptypevalues securitiesAllocationWorkstepType) {
        this.securitiesAllocationWorkstepType = securitiesAllocationWorkstepType;
    }

    public org.museframework.bian.enumerations.Securitiesallocationworksteptypevalues getSecuritiesAllocationWorkstepType() {
        return securitiesAllocationWorkstepType;
    }
}