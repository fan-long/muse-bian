package org.museframework.bian.classes;

public class RiskAssessment {
    /*The type of risk assessed

e.g. currency risk, credit risk, reputation risk*/
    private String riskAssessmentViewpoint;

    public void setRiskAssessmentViewpoint(String riskAssessmentViewpoint) {
        this.riskAssessmentViewpoint = riskAssessmentViewpoint;
    }

    public String getRiskAssessmentViewpoint() {
        return riskAssessmentViewpoint;
    }
}