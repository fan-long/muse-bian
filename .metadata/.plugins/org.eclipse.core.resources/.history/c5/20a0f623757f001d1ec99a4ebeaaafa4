/*Right or obligation linked to the involvement of a party in an agreement

Can be default, linked to an involvement type, or specifically agreed upon. 
This can be default obligations or entitlements, of specific arrangements*/
package org.museframework.bian.classes;

public class PartyObligationEntitlement {
    /**/
    private String obligationEntitlementSubject;

    /**/
    private String obligationEntitlementType;

    /**/
    private String obligationEntitlementModality;

    public void setObligationEntitlementSubject(String obligationEntitlementSubject) {
        this.obligationEntitlementSubject = obligationEntitlementSubject;
    }

    public String getObligationEntitlementSubject() {
        return obligationEntitlementSubject;
    }

    public void setObligationEntitlementType(String obligationEntitlementType) {
        this.obligationEntitlementType = obligationEntitlementType;
    }

    public String getObligationEntitlementType() {
        return obligationEntitlementType;
    }

    public void setObligationEntitlementModality(String obligationEntitlementModality) {
        this.obligationEntitlementModality = obligationEntitlementModality;
    }

    public String getObligationEntitlementModality() {
        return obligationEntitlementModality;
    }
}