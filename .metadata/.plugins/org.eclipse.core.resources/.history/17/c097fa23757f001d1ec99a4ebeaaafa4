/*Human entity, as distinguished from a corporate entity (which is sometimes referred to as an 'artificial person'). ISO20022*/
package org.museframework.bian.classes;

public class Person {
    /**/
    private String personIdentification;

    /*Residential status of an individual, for example, non-permanent resident. (ISO20022)*/
    private String residentialStatus;

    /*Date on which a person is born. (ISO20022)*/
    private String birthDate;

    /*Specifies the country where a person was born or is legally accepted as belonging to the country. (ISO20022)*/
    private String nationality;

    /*The religious identification of the person.*/
    private String ethnicity;

    /**/
    private String religion;

    /**/
    private String personName;

    public void setPersonIdentification(String personIdentification) {
        this.personIdentification = personIdentification;
    }

    public String getPersonIdentification() {
        return personIdentification;
    }

    public void setResidentialStatus(String residentialStatus) {
        this.residentialStatus = residentialStatus;
    }

    public String getResidentialStatus() {
        return residentialStatus;
    }

    public void setBirthDate(String birthDate) {
        this.birthDate = birthDate;
    }

    public String getBirthDate() {
        return birthDate;
    }

    public void setNationality(String nationality) {
        this.nationality = nationality;
    }

    public String getNationality() {
        return nationality;
    }

    public void setEthnicity(String ethnicity) {
        this.ethnicity = ethnicity;
    }

    public String getEthnicity() {
        return ethnicity;
    }

    public void setReligion(String religion) {
        this.religion = religion;
    }

    public String getReligion() {
        return religion;
    }

    public void setPersonName(String personName) {
        this.personName = personName;
    }

    public String getPersonName() {
        return personName;
    }
}