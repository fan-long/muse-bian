/*Party who holds a payment account and allows a payment order from that payment account, or, where there is no payment account, a party who gives a payment order;*/
package org.museframework.bian.classes;

public class Payer {
    /*Identifier of the payer.*/
    private String payerIdentifier;

    public void setPayerIdentifier(String payerIdentifier) {
        this.payerIdentifier = payerIdentifier;
    }

    public String getPayerIdentifier() {
        return payerIdentifier;
    }
}