/*The meaning of a time indication for an object; E.g. opening data of an account, signing date of an agreement... birth date of a person.*/
package org.museframework.bian.classes;

public class RelevantDateTime {
    /*A classification of the relevance a time indication has for an object.*/
    private String dateTimeRelevanceType;

    public void setDateTimeRelevanceType(String dateTimeRelevanceType) {
        this.dateTimeRelevanceType = dateTimeRelevanceType;
    }

    public String getDateTimeRelevanceType() {
        return dateTimeRelevanceType;
    }
}