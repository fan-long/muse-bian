/*An evaluation whether a party or system is what it claims to be.

A security mechanism for verifying: 1) the identity of an individual or other entity (including verification by means of a computer or computer application); and 2) the level of authority of that person or entity (i.e. the ability of that person or entity to perform specific tasks or activities). (ECB)*/
package org.museframework.bian.classes;

public class Authentication {
    /**/
    private String authenticationLevel;

    /*Entity or object in charge of verifying the person authenticity.*/
    private String authenticationEntity;

    /**/
    private org.museframework.bian.enumerations.Authenticationmethodtypevalues authenticationMethodType;

    /**/
    private String authenticationValue;

    /*Specifies the result of the authentication.*/
    private org.museframework.bian.enumerations.Authenticationresulttypevalues authenticationResult;

    public void setAuthenticationLevel(String authenticationLevel) {
        this.authenticationLevel = authenticationLevel;
    }

    public String getAuthenticationLevel() {
        return authenticationLevel;
    }

    public void setAuthenticationEntity(String authenticationEntity) {
        this.authenticationEntity = authenticationEntity;
    }

    public String getAuthenticationEntity() {
        return authenticationEntity;
    }

    public void setAuthenticationMethodType(org.museframework.bian.enumerations.Authenticationmethodtypevalues authenticationMethodType) {
        this.authenticationMethodType = authenticationMethodType;
    }

    public org.museframework.bian.enumerations.Authenticationmethodtypevalues getAuthenticationMethodType() {
        return authenticationMethodType;
    }

    public void setAuthenticationValue(String authenticationValue) {
        this.authenticationValue = authenticationValue;
    }

    public String getAuthenticationValue() {
        return authenticationValue;
    }

    public void setAuthenticationResult(org.museframework.bian.enumerations.Authenticationresulttypevalues authenticationResult) {
        this.authenticationResult = authenticationResult;
    }

    public org.museframework.bian.enumerations.Authenticationresulttypevalues getAuthenticationResult() {
        return authenticationResult;
    }
}