/*The action/task which is planned to develop the customer relationship. */
package org.museframework.bian.classes;

public class CustomerRelationshipDevelopment {
    /**/
    private String customerRelationshipDevelopmentTaskType;

    /**/
    private String customerRelationshipDevelopmentTaskDescription;

    /**/
    private String customerRelationshipDevelopmentTaskResult;

    public void setCustomerRelationshipDevelopmentTaskType(String customerRelationshipDevelopmentTaskType) {
        this.customerRelationshipDevelopmentTaskType = customerRelationshipDevelopmentTaskType;
    }

    public String getCustomerRelationshipDevelopmentTaskType() {
        return customerRelationshipDevelopmentTaskType;
    }

    public void setCustomerRelationshipDevelopmentTaskDescription(String customerRelationshipDevelopmentTaskDescription) {
        this.customerRelationshipDevelopmentTaskDescription = customerRelationshipDevelopmentTaskDescription;
    }

    public String getCustomerRelationshipDevelopmentTaskDescription() {
        return customerRelationshipDevelopmentTaskDescription;
    }

    public void setCustomerRelationshipDevelopmentTaskResult(String customerRelationshipDevelopmentTaskResult) {
        this.customerRelationshipDevelopmentTaskResult = customerRelationshipDevelopmentTaskResult;
    }

    public String getCustomerRelationshipDevelopmentTaskResult() {
        return customerRelationshipDevelopmentTaskResult;
    }
}