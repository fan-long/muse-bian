/*A state of affairs during a dunning procedure */
package org.museframework.bian.classes;

public class DunningAnalysis {
    /**/
    private String dunningAnalysisIdentification;

    /**/
    private String dunningProcedure;

    /*Date for which the dunning analysis is valid*/
    private String dunningAnalysisDate;

    /*Recording of the analysis actions*/
    private String dunningAnalysisLog;

    public void setDunningAnalysisIdentification(String dunningAnalysisIdentification) {
        this.dunningAnalysisIdentification = dunningAnalysisIdentification;
    }

    public String getDunningAnalysisIdentification() {
        return dunningAnalysisIdentification;
    }

    public void setDunningProcedure(String dunningProcedure) {
        this.dunningProcedure = dunningProcedure;
    }

    public String getDunningProcedure() {
        return dunningProcedure;
    }

    public void setDunningAnalysisDate(String dunningAnalysisDate) {
        this.dunningAnalysisDate = dunningAnalysisDate;
    }

    public String getDunningAnalysisDate() {
        return dunningAnalysisDate;
    }

    public void setDunningAnalysisLog(String dunningAnalysisLog) {
        this.dunningAnalysisLog = dunningAnalysisLog;
    }

    public String getDunningAnalysisLog() {
        return dunningAnalysisLog;
    }
}