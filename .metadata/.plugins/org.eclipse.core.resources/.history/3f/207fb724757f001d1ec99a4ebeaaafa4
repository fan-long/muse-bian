/*Set of integrated applications that provides centralised services such as clearing, netting, reconciliation, trading and/or settlement. (ISO20022)*/
package org.museframework.bian.classes;

public class System {
    /*The time schedule that system services are available for use.*/
    private String systemAccessSchedule;

    /**/
    private String systemType;

    public void setSystemAccessSchedule(String systemAccessSchedule) {
        this.systemAccessSchedule = systemAccessSchedule;
    }

    public String getSystemAccessSchedule() {
        return systemAccessSchedule;
    }

    public void setSystemType(String systemType) {
        this.systemType = systemType;
    }

    public String getSystemType() {
        return systemType;
    }
}