/*A specific role of a specific Party in the location.
*/
package org.museframework.bian.classes;

public class LocationInvolvement {
    /**/
    private org.museframework.bian.enumerations.Locationinvolvementtypevalues locationInvolvementType;

    /**/
    private Location locationReference;

    /**/
    private Party partyReference;

    public void setLocationInvolvementType(org.museframework.bian.enumerations.Locationinvolvementtypevalues locationInvolvementType) {
        this.locationInvolvementType = locationInvolvementType;
    }

    public org.museframework.bian.enumerations.Locationinvolvementtypevalues getLocationInvolvementType() {
        return locationInvolvementType;
    }

    public void setLocationReference(Location locationReference) {
        this.locationReference = locationReference;
    }

    public Location getLocationReference() {
        return locationReference;
    }

    public void setPartyReference(Party partyReference) {
        this.partyReference = partyReference;
    }

    public Party getPartyReference() {
        return partyReference;
    }
}