/*Instruction to execute a payment from a payer to a payee*/
package org.museframework.bian.classes;

public class MoneyTransferInstruction {
    /*Classification of the Money Transfer Instruction according to the event that lead to the instruction

e.g. withdrawal, interest disbursement, direct debit, card payment.*/
    private String moneyTransferInstructionType;

    /*Amount of money to be transferred.*/
    private String moneyTransferInstructionAmount;

    /*Currency in which the transfer amount is expressed.*/
    private String moneyTransferInstructionAmountCurrency;

    /*The reason why the instruction is given

can be reference to a lot of information, concerning e.g. a purchase, an arrangement fulfillment event, a billing period...*/
    private String moneyTransferInstructionPurpose;

    /*The network to be used for the processing of the money transfer instruction as requested in the instruction.
e.g. instant SEPA?*/
    private String moneyTransferInstructionRequestedMechanism;

    /*The processing status of the instruction*/
    private String moneyTransferInstructionStatus;

    /*Reference to the contact channel that was used to give the money transfer instruction*/
    private String moneyTransferInstructionChannelReference;

    public void setMoneyTransferInstructionType(String moneyTransferInstructionType) {
        this.moneyTransferInstructionType = moneyTransferInstructionType;
    }

    public String getMoneyTransferInstructionType() {
        return moneyTransferInstructionType;
    }

    public void setMoneyTransferInstructionAmount(String moneyTransferInstructionAmount) {
        this.moneyTransferInstructionAmount = moneyTransferInstructionAmount;
    }

    public String getMoneyTransferInstructionAmount() {
        return moneyTransferInstructionAmount;
    }

    public void setMoneyTransferInstructionAmountCurrency(String moneyTransferInstructionAmountCurrency) {
        this.moneyTransferInstructionAmountCurrency = moneyTransferInstructionAmountCurrency;
    }

    public String getMoneyTransferInstructionAmountCurrency() {
        return moneyTransferInstructionAmountCurrency;
    }

    public void setMoneyTransferInstructionPurpose(String moneyTransferInstructionPurpose) {
        this.moneyTransferInstructionPurpose = moneyTransferInstructionPurpose;
    }

    public String getMoneyTransferInstructionPurpose() {
        return moneyTransferInstructionPurpose;
    }

    public void setMoneyTransferInstructionRequestedMechanism(String moneyTransferInstructionRequestedMechanism) {
        this.moneyTransferInstructionRequestedMechanism = moneyTransferInstructionRequestedMechanism;
    }

    public String getMoneyTransferInstructionRequestedMechanism() {
        return moneyTransferInstructionRequestedMechanism;
    }

    public void setMoneyTransferInstructionStatus(String moneyTransferInstructionStatus) {
        this.moneyTransferInstructionStatus = moneyTransferInstructionStatus;
    }

    public String getMoneyTransferInstructionStatus() {
        return moneyTransferInstructionStatus;
    }

    public void setMoneyTransferInstructionChannelReference(String moneyTransferInstructionChannelReference) {
        this.moneyTransferInstructionChannelReference = moneyTransferInstructionChannelReference;
    }

    public String getMoneyTransferInstructionChannelReference() {
        return moneyTransferInstructionChannelReference;
    }
}