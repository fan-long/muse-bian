/*A payment service to execute payment transactions through a payment card, including online payments. (Adapted from PSD)

A service that offers customers the use of payment cards.*/
package org.museframework.bian.classes;

public class CardPaymentService {
    /**/
    private String cardPaymentServiceIdentifier;

    /*Type of payment card as a product.*/
    private org.museframework.bian.enumerations.Cardpaymentservicetypevalues cardPaymentServiceType;

    /*Configuration specified for the payment card product.*/
    private String cardPaymentServiceConfiguration;

    public void setCardPaymentServiceIdentifier(String cardPaymentServiceIdentifier) {
        this.cardPaymentServiceIdentifier = cardPaymentServiceIdentifier;
    }

    public String getCardPaymentServiceIdentifier() {
        return cardPaymentServiceIdentifier;
    }

    public void setCardPaymentServiceType(org.museframework.bian.enumerations.Cardpaymentservicetypevalues cardPaymentServiceType) {
        this.cardPaymentServiceType = cardPaymentServiceType;
    }

    public org.museframework.bian.enumerations.Cardpaymentservicetypevalues getCardPaymentServiceType() {
        return cardPaymentServiceType;
    }

    public void setCardPaymentServiceConfiguration(String cardPaymentServiceConfiguration) {
        this.cardPaymentServiceConfiguration = cardPaymentServiceConfiguration;
    }

    public String getCardPaymentServiceConfiguration() {
        return cardPaymentServiceConfiguration;
    }
}