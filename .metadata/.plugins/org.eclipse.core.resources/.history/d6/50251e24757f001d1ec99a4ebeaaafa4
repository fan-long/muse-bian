/*A specification providing information about a designed object (e.g. product, service, process, procedure, model) covering all aspects required for its use.

*/
package org.museframework.bian.classes;

public class DesignSpecification {
    /*The operational and development status of a design specification (e.g. under development, available, in-use under review) */
    private String designSpecificationStatus;

    /*Release version of the available design specification plus version history as appropriate*/
    private String designSpecificationVersion;

    /**/
    private String designSpecificationType;

    /**/
    private String designSpecificationDescription;

    public void setDesignSpecificationStatus(String designSpecificationStatus) {
        this.designSpecificationStatus = designSpecificationStatus;
    }

    public String getDesignSpecificationStatus() {
        return designSpecificationStatus;
    }

    public void setDesignSpecificationVersion(String designSpecificationVersion) {
        this.designSpecificationVersion = designSpecificationVersion;
    }

    public String getDesignSpecificationVersion() {
        return designSpecificationVersion;
    }

    public void setDesignSpecificationType(String designSpecificationType) {
        this.designSpecificationType = designSpecificationType;
    }

    public String getDesignSpecificationType() {
        return designSpecificationType;
    }

    public void setDesignSpecificationDescription(String designSpecificationDescription) {
        this.designSpecificationDescription = designSpecificationDescription;
    }

    public String getDesignSpecificationDescription() {
        return designSpecificationDescription;
    }
}