/*An Operating Session using an e-Branch Channel*/
package org.museframework.bian.classes;

public class EbranchChannelSession {
    /*Reference to the e-Branch Channel Service Menu used in the session*/
    private String ebranchSessionServiceMenu;

    /*Indication of where the EBranchChannelSession was originated (inbound or outbound)*/
    private String ebranchChannelSessionDirection;

    /*Refernce to the Device used to receive access to the EBranchChannel Session

in case an authorisation is required*/
    private Object ebranchChannelSessionAccessDeviceReference;

    /*Reference to a device used  the EBranchChannelSession 

needs to be understood combined with "role", e.g. authentication, authorisation, connection... */
    private Object ebranchChannelSessionDeviceReference;

    /*The role a device plays in a channelsession

e.g. authentication, authorisation, connection... 
*/
    private String ebranchSessionDeviceRole;

    public void setEbranchSessionServiceMenu(String ebranchSessionServiceMenu) {
        this.ebranchSessionServiceMenu = ebranchSessionServiceMenu;
    }

    public String getEbranchSessionServiceMenu() {
        return ebranchSessionServiceMenu;
    }

    public void setEbranchChannelSessionDirection(String ebranchChannelSessionDirection) {
        this.ebranchChannelSessionDirection = ebranchChannelSessionDirection;
    }

    public String getEbranchChannelSessionDirection() {
        return ebranchChannelSessionDirection;
    }

    public void setEbranchChannelSessionAccessDeviceReference(Object ebranchChannelSessionAccessDeviceReference) {
        this.ebranchChannelSessionAccessDeviceReference = ebranchChannelSessionAccessDeviceReference;
    }

    public Object getEbranchChannelSessionAccessDeviceReference() {
        return ebranchChannelSessionAccessDeviceReference;
    }

    public void setEbranchChannelSessionDeviceReference(Object ebranchChannelSessionDeviceReference) {
        this.ebranchChannelSessionDeviceReference = ebranchChannelSessionDeviceReference;
    }

    public Object getEbranchChannelSessionDeviceReference() {
        return ebranchChannelSessionDeviceReference;
    }

    public void setEbranchSessionDeviceRole(String ebranchSessionDeviceRole) {
        this.ebranchSessionDeviceRole = ebranchSessionDeviceRole;
    }

    public String getEbranchSessionDeviceRole() {
        return ebranchSessionDeviceRole;
    }
}