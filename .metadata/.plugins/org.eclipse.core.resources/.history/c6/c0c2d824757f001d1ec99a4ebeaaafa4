/*A specific party who plays a specific role in the document. */
package org.museframework.bian.classes;

public class DocumentInvolvement {
    /**/
    private org.museframework.bian.enumerations.Documentinvolvementtypevalues documentInvolvementType;

    /**/
    private Document documentReference;

    /**/
    private Party partyReference;

    public void setDocumentInvolvementType(org.museframework.bian.enumerations.Documentinvolvementtypevalues documentInvolvementType) {
        this.documentInvolvementType = documentInvolvementType;
    }

    public org.museframework.bian.enumerations.Documentinvolvementtypevalues getDocumentInvolvementType() {
        return documentInvolvementType;
    }

    public void setDocumentReference(Document documentReference) {
        this.documentReference = documentReference;
    }

    public Document getDocumentReference() {
        return documentReference;
    }

    public void setPartyReference(Party partyReference) {
        this.partyReference = partyReference;
    }

    public Party getPartyReference() {
        return partyReference;
    }
}