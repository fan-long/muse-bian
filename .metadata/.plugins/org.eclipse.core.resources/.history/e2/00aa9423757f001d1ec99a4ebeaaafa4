/*The involvement (role) of an instance of a party in an instance of a product.*/
package org.museframework.bian.classes;

public class ProductInvolvement {
    /**/
    private org.museframework.bian.enumerations.Productinvolvementtypevalues productInvolvementType;

    /**/
    private Party partyReference;

    public void setProductInvolvementType(org.museframework.bian.enumerations.Productinvolvementtypevalues productInvolvementType) {
        this.productInvolvementType = productInvolvementType;
    }

    public org.museframework.bian.enumerations.Productinvolvementtypevalues getProductInvolvementType() {
        return productInvolvementType;
    }

    public void setPartyReference(Party partyReference) {
        this.partyReference = partyReference;
    }

    public Party getPartyReference() {
        return partyReference;
    }
}