/*Arrangement within a Bank Guarantee Agreement*/
package org.museframework.bian.classes;

public class BankGuaranteeArrangement {
    /**/
    private org.museframework.bian.enumerations.Bankguaranteearrangementtypevalues bankGuaranteeArrangementType;

    /*Amount covered by the guarantee. (ISO20022)*/
    private String coveredAmount;

    /*Period during which the guarantee is valid. (ISO20022)*/
    private String effectivePeriod;

    /*Amount not covered by the guarantee. (ISO20022)*/
    private String excessAmount;

    /*Key dates of a guarantee like ExpiryDate, MaturityDate, */
    private String guaranteeDate;

    public void setBankGuaranteeArrangementType(org.museframework.bian.enumerations.Bankguaranteearrangementtypevalues bankGuaranteeArrangementType) {
        this.bankGuaranteeArrangementType = bankGuaranteeArrangementType;
    }

    public org.museframework.bian.enumerations.Bankguaranteearrangementtypevalues getBankGuaranteeArrangementType() {
        return bankGuaranteeArrangementType;
    }

    public void setCoveredAmount(String coveredAmount) {
        this.coveredAmount = coveredAmount;
    }

    public String getCoveredAmount() {
        return coveredAmount;
    }

    public void setEffectivePeriod(String effectivePeriod) {
        this.effectivePeriod = effectivePeriod;
    }

    public String getEffectivePeriod() {
        return effectivePeriod;
    }

    public void setExcessAmount(String excessAmount) {
        this.excessAmount = excessAmount;
    }

    public String getExcessAmount() {
        return excessAmount;
    }

    public void setGuaranteeDate(String guaranteeDate) {
        this.guaranteeDate = guaranteeDate;
    }

    public String getGuaranteeDate() {
        return guaranteeDate;
    }
}