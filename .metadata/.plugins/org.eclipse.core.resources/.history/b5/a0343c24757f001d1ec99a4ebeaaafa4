/*A Procedure to settle a market trade

Process which consists in transferring the proceeds related to a trade from one party to the next one. ISO20022*/
package org.museframework.bian.classes;

public class MarketTradeSettlement {
    /**/
    private Workstep marketTradeSettlementWorkstep;

    /**/
    private org.museframework.bian.enumerations.Markettradesettlementworksteptype marketTradeSettlementWorkstepType;

    public void setMarketTradeSettlementWorkstep(Workstep marketTradeSettlementWorkstep) {
        this.marketTradeSettlementWorkstep = marketTradeSettlementWorkstep;
    }

    public Workstep getMarketTradeSettlementWorkstep() {
        return marketTradeSettlementWorkstep;
    }

    public void setMarketTradeSettlementWorkstepType(org.museframework.bian.enumerations.Markettradesettlementworksteptype marketTradeSettlementWorkstepType) {
        this.marketTradeSettlementWorkstepType = marketTradeSettlementWorkstepType;
    }

    public org.museframework.bian.enumerations.Markettradesettlementworksteptype getMarketTradeSettlementWorkstepType() {
        return marketTradeSettlementWorkstepType;
    }
}