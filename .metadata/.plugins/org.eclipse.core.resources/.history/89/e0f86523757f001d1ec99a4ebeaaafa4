/*A workstep to be followed in the execution of a product training procedure. */
package org.museframework.bian.classes;

public class ProductTrainingWorkstep {
    /**/
    private String productTrainingWorkstepType;

    public void setProductTrainingWorkstepType(String productTrainingWorkstepType) {
        this.productTrainingWorkstepType = productTrainingWorkstepType;
    }

    public String getProductTrainingWorkstepType() {
        return productTrainingWorkstepType;
    }
}