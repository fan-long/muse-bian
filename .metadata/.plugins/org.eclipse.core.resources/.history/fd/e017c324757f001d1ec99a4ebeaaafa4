/*A party role which is involved in the payment transaction.
*/
package org.museframework.bian.classes;

public class PaymentTransactionInvolvement {
    /*The role that a party plays in a payment transaction.*/
    private org.museframework.bian.enumerations.Paymenttransactioninvolvementtypevalues paymentTransactionInvolvementType;

    public void setPaymentTransactionInvolvementType(org.museframework.bian.enumerations.Paymenttransactioninvolvementtypevalues paymentTransactionInvolvementType) {
        this.paymentTransactionInvolvementType = paymentTransactionInvolvementType;
    }

    public org.museframework.bian.enumerations.Paymenttransactioninvolvementtypevalues getPaymentTransactionInvolvementType() {
        return paymentTransactionInvolvementType;
    }
}