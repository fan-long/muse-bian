/*A detailed characteristic of a product expressed in offered services and the terms and conditions applicable to the delivery of the services. */
package org.museframework.bian.classes;

public class ProductFeature {
    /*The type or category of product/service features*/
    private String productFeatureType;

    /**/
    private String productFeatureSpecification;

    public void setProductFeatureType(String productFeatureType) {
        this.productFeatureType = productFeatureType;
    }

    public String getProductFeatureType() {
        return productFeatureType;
    }

    public void setProductFeatureSpecification(String productFeatureSpecification) {
        this.productFeatureSpecification = productFeatureSpecification;
    }

    public String getProductFeatureSpecification() {
        return productFeatureSpecification;
    }
}