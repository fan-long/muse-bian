/*An act, initiated by the payer or payee, of placing, transferring or withdrawing funds, irrespective of any underlying obligations between the payer and payee (a proposed definition for BIAN BOM adapted from PSD)

*/
package org.museframework.bian.classes;

public class PaymentTransaction {
    /**/
    private org.museframework.bian.enumerations.Paymenttransactiontypevalues paymentTransactionType;

    /**/
    private String paymentTransactionAmount;

    /*An identifier that allows to uniquely refer to a Payment Transaction.*/
    private String paymentTransactionIdentifier;

    /*The payment mechanism the bank decides to use in the transaction to execute a payment instruction */
    private String paymentTransactionUsedMechanism;

    /**/
    private String paymentTransactionDateTime;

    /**/
    private String paymentTransactionPurpose;

    /**/
    private String paymentTransactionStatus;

    /**/
    private String paymentTransactionAppliedFee;

    /**/
    private String paymentTransactionCurrency;

    public void setPaymentTransactionType(org.museframework.bian.enumerations.Paymenttransactiontypevalues paymentTransactionType) {
        this.paymentTransactionType = paymentTransactionType;
    }

    public org.museframework.bian.enumerations.Paymenttransactiontypevalues getPaymentTransactionType() {
        return paymentTransactionType;
    }

    public void setPaymentTransactionAmount(String paymentTransactionAmount) {
        this.paymentTransactionAmount = paymentTransactionAmount;
    }

    public String getPaymentTransactionAmount() {
        return paymentTransactionAmount;
    }

    public void setPaymentTransactionIdentifier(String paymentTransactionIdentifier) {
        this.paymentTransactionIdentifier = paymentTransactionIdentifier;
    }

    public String getPaymentTransactionIdentifier() {
        return paymentTransactionIdentifier;
    }

    public void setPaymentTransactionUsedMechanism(String paymentTransactionUsedMechanism) {
        this.paymentTransactionUsedMechanism = paymentTransactionUsedMechanism;
    }

    public String getPaymentTransactionUsedMechanism() {
        return paymentTransactionUsedMechanism;
    }

    public void setPaymentTransactionDateTime(String paymentTransactionDateTime) {
        this.paymentTransactionDateTime = paymentTransactionDateTime;
    }

    public String getPaymentTransactionDateTime() {
        return paymentTransactionDateTime;
    }

    public void setPaymentTransactionPurpose(String paymentTransactionPurpose) {
        this.paymentTransactionPurpose = paymentTransactionPurpose;
    }

    public String getPaymentTransactionPurpose() {
        return paymentTransactionPurpose;
    }

    public void setPaymentTransactionStatus(String paymentTransactionStatus) {
        this.paymentTransactionStatus = paymentTransactionStatus;
    }

    public String getPaymentTransactionStatus() {
        return paymentTransactionStatus;
    }

    public void setPaymentTransactionAppliedFee(String paymentTransactionAppliedFee) {
        this.paymentTransactionAppliedFee = paymentTransactionAppliedFee;
    }

    public String getPaymentTransactionAppliedFee() {
        return paymentTransactionAppliedFee;
    }

    public void setPaymentTransactionCurrency(String paymentTransactionCurrency) {
        this.paymentTransactionCurrency = paymentTransactionCurrency;
    }

    public String getPaymentTransactionCurrency() {
        return paymentTransactionCurrency;
    }
}