/*The instruction concerning to take money from an account.*/
package org.museframework.bian.classes;

public class WithdrawalInstruction {
    /*Reference to how, by whom and in what "materialisation" the withdrawal is performed.

e.g. as a bank cheque, in cash*/
    private Object withdrawalTargetReference;

    /*The type of withdrawal transaction that is applied to the account (e.g. customer withdrawal, internal service charge, disbursement)


Type of the withdrawal
e.g. reservation, actual withdrawal ???*/
    private String withdrawalType;

    /*Information about the purpose or other reference information accompanying the withdrawal.*/
    private String withdrawalMessage;

    /*The withdrawn amount*/
    private String withdrawalAmount;

    /*The Date(time) on which the withdrawal is initiated.*/
    private String withdrawalInstructionDate;

    public void setWithdrawalTargetReference(Object withdrawalTargetReference) {
        this.withdrawalTargetReference = withdrawalTargetReference;
    }

    public Object getWithdrawalTargetReference() {
        return withdrawalTargetReference;
    }

    public void setWithdrawalType(String withdrawalType) {
        this.withdrawalType = withdrawalType;
    }

    public String getWithdrawalType() {
        return withdrawalType;
    }

    public void setWithdrawalMessage(String withdrawalMessage) {
        this.withdrawalMessage = withdrawalMessage;
    }

    public String getWithdrawalMessage() {
        return withdrawalMessage;
    }

    public void setWithdrawalAmount(String withdrawalAmount) {
        this.withdrawalAmount = withdrawalAmount;
    }

    public String getWithdrawalAmount() {
        return withdrawalAmount;
    }

    public void setWithdrawalInstructionDate(String withdrawalInstructionDate) {
        this.withdrawalInstructionDate = withdrawalInstructionDate;
    }

    public String getWithdrawalInstructionDate() {
        return withdrawalInstructionDate;
    }
}