/*Algorithm used for Market Analysis*/
package org.museframework.bian.classes;

public class MarketAnalysisAlgorithm {
    /*Classification of the algorithm

e.g. regression*/
    private String marketAnalysisAlgorithmType;

    /*Definition of the algorithm 

such as required inputs, the internal data, calculations and outputs*/
    private String marketAnalysisAlgorithmSpecification;

    /*Guidance on the use of the algorithm and interpretation of its results*/
    private String marketAnalysisAlgorithmGuidance;

    public void setMarketAnalysisAlgorithmType(String marketAnalysisAlgorithmType) {
        this.marketAnalysisAlgorithmType = marketAnalysisAlgorithmType;
    }

    public String getMarketAnalysisAlgorithmType() {
        return marketAnalysisAlgorithmType;
    }

    public void setMarketAnalysisAlgorithmSpecification(String marketAnalysisAlgorithmSpecification) {
        this.marketAnalysisAlgorithmSpecification = marketAnalysisAlgorithmSpecification;
    }

    public String getMarketAnalysisAlgorithmSpecification() {
        return marketAnalysisAlgorithmSpecification;
    }

    public void setMarketAnalysisAlgorithmGuidance(String marketAnalysisAlgorithmGuidance) {
        this.marketAnalysisAlgorithmGuidance = marketAnalysisAlgorithmGuidance;
    }

    public String getMarketAnalysisAlgorithmGuidance() {
        return marketAnalysisAlgorithmGuidance;
    }
}