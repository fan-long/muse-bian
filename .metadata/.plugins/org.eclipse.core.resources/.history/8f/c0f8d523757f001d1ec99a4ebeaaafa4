/*A high level plan to achieve one or more goals */
package org.museframework.bian.classes;

public class Strategy {
    /*he time period addressed by the strategy*/
    private String strategyPeriod;

    /*Target and actual financial budget for the strategy*/
    private String strategyBudget;

    /*The target and actual strategy implementation schedule - recording planned and actual initiatives associated with the realization of the strategy*/
    private String strategySchedule;

    public void setStrategyPeriod(String strategyPeriod) {
        this.strategyPeriod = strategyPeriod;
    }

    public String getStrategyPeriod() {
        return strategyPeriod;
    }

    public void setStrategyBudget(String strategyBudget) {
        this.strategyBudget = strategyBudget;
    }

    public String getStrategyBudget() {
        return strategyBudget;
    }

    public void setStrategySchedule(String strategySchedule) {
        this.strategySchedule = strategySchedule;
    }

    public String getStrategySchedule() {
        return strategySchedule;
    }
}