/*Execution of the sweep arrangement */
package org.museframework.bian.classes;

public class SweepArrangementFulfillmentEvent {
    /**/
    private String sweepEventCounterAccountReference;

    /*The account in which the sweep amount came from or went to*/
    private String sweepEventAmount;

    /*The date on which the sweep transfer was executed*/
    private String sweepEventExecutionDate;

    public void setSweepEventCounterAccountReference(String sweepEventCounterAccountReference) {
        this.sweepEventCounterAccountReference = sweepEventCounterAccountReference;
    }

    public String getSweepEventCounterAccountReference() {
        return sweepEventCounterAccountReference;
    }

    public void setSweepEventAmount(String sweepEventAmount) {
        this.sweepEventAmount = sweepEventAmount;
    }

    public String getSweepEventAmount() {
        return sweepEventAmount;
    }

    public void setSweepEventExecutionDate(String sweepEventExecutionDate) {
        this.sweepEventExecutionDate = sweepEventExecutionDate;
    }

    public String getSweepEventExecutionDate() {
        return sweepEventExecutionDate;
    }
}